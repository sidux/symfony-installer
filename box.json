{
    "directories": ["src/"],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": "vendor"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "git-version": "package_version",
    "main": "aw",
    "output": "aw.phar",
    "stub": true,
    "chmod": "0755"
}
